    "eslint": "~1.10.1",
    "eslint-config-standard": "~4.4.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-smells": "~1.0.1",
    "eslint-plugin-standard": "~1.3.1",
    "faucet": "0.0.1",
    "tap-difflet": "~0.4.0",

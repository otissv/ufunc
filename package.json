{
  "name": "ufunc",
  "version": "3.0.0",
  "description": "Ufunc is a set of Typescript and JavaScript functional methods",
  "keywords": [
    "javascript",
    "typescript",
    "functional programming"
  ],
  "repository": "git@github.com:otissv/ufunc.git",
  "author": "Otis Virginie",
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && mkdir dist && node ./scripts",
    "watch": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:coverage": "npm run build && jest --coverage",
    "test:watch": "jest --watch",
    "docs": "npm run build && typedoc src/index.ts",
    "generate": "npm run clean && mkdir dist  && node ../scripts",
    "clean": "rimraf ./dist",
    "prepare": "npm run generate",
    "publish": "cd dist && npm publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint . --ext .ts"
    }
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-disable": "^2.0.1",
    "eslint-plugin-functional": "^3.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.7",
    "typescript": "^4.1.3"
  }
}
